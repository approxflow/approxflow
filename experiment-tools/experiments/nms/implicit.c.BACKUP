int implicit(int input) {
  int output = 0;
  if (input == 0) output = 0;
  else if (input == 1) output = 1;
  else if (input == 2) output = 2;
  else if (input == 3) output = 3;
  else if (input == 4) output = 4;
  else if (input == 5) output = 5;
  else if (input == 6) output = 6;
  else output = 0;
  return output;
}
